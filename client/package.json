{
  "name": "angularjs-breeze-boot",
  "version": "0.0.1",
  "description": "An example of application, using: AngularJS 1.5, Webpack, ES6, Spring Boot",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server -w --hot --inline --config config/dev.babel.js --content-base=./src/",
    "prebuild": "rimraf -rf ../src/main/resources/static",
    "watch": "npm run compile -- -w",
    "compile": "webpack --config config/dev.babel.js",
    "build": "webpack --config config/prod.babel.js -p"
  },
  "babel": {
    "presets": [
      "stage-0",
      "es2015"
    ],
    "plugins": [
      "add-module-exports"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.16.0",
    "babel-loader": "^6.2.5",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.14.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.16.3",
    "cssnano": "^3.8.1",
    "css-loader": "^0.26.1",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "^2.22.0",
    "ncp": "^2.0.0",
    "ng-annotate-loader": "^0.2.0",
    "npm-run-all": "^3.1.0",
    "postcss": "^5.2.6",
    "postcss-loader": "^1.2.0",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.4",
    "script-ext-html-webpack-plugin": "^1.3.4",
    "source-map-loader": "^0.1.5",
    "style-loader": "^0.13.1",
    "stylus": "^0.54.5",
    "stylus-loader": "^2.3.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.2",
    "webpack-dev-server": "^1.16.1",
    "webpack-md5-hash": "0.0.5"
  },
  "dependencies": {
    "angular": "^1.5.8",
    "angular-route": "^1.5.8",
    "angular-ui-router": "^0.3.1",
    "bootstrap": "^3.3.7",
    "jquery": "^3.1.1"
  },
  "keywords": [
    "angular",
    "webpack",
    "babel"
  ],
  "author": "daggerok",
  "license": "ISC",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/angularjs-webpack-es6-spring-boot.git"
  },
  "bugs": {
    "url": "https://github.com/daggerok/angularjs-webpack-es6-spring-boot/issues"
  },
  "homepage": "https://github.com/daggerok/angularjs-webpack-es6-spring-boot#readme"
}
